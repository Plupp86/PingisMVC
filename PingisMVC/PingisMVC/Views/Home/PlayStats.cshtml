@model PlayerStatsVM

@foreach (var item in Model.recentMatches)
	{
<tr class="stats">
	<td colspan="7">@item.DatePlayed.ToString("yyyy-MM-dd"): <span class="@((@item.SetsOne > @item.SetsTwo)?"win":"lose")">@item.PlayerOne</span> - <span class="@((@item.SetsOne < @item.SetsTwo)?"win":"lose")">@item.PlayerTwo</span>   <strong>@item.SetsOne - @item.SetsTwo </strong></td>
</tr>
	}
